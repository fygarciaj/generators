{
    "name": "fygarciaj/generators",
    "description": "Custom Laravel File Generators with config and publishable stubs.",
    "keywords": [
        "laravel",
        "generators",
        "stubs",
        "config",
        "commands",
        "customization"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Fabian Garcia",
            "email": "fygarciaj@gmail.com",
            "homepage": "http://fygarciaj.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/console": "^6.0",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Fygarciaj\\Generators\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fygarciaj\\Generators\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fygarciaj\\Generators\\GeneratorsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
